<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycoolproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.mycoolproject.MainActivity"
            android:configChanges="orientation"
            android:label="@string/app_name" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.mycoolproject.LoginActivity"
            android:label="@string/title_activity_login" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.mycoolproject.ListViewActivity"
            android:configChanges="orientation"
            android:label="@string/title_activity_list_view" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.mycoolproject.CameraActivity"
            android:label="@string/title_activity_camera" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.mycoolproject.CommentActivity"
            android:label="@string/title_activity_comment" android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
